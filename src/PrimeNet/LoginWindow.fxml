<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PrimeNet.Controller">
   <children>
      <Button layoutX="34.0" layoutY="234.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="TextUserName" layoutX="34.0" layoutY="75.0" promptText="Benutzername">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="TextPassword" layoutX="34.0" layoutY="133.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Label fx:id="LabelStatus" layoutX="40.0" layoutY="32.0" maxHeight="23.0" maxWidth="500.0" minHeight="23.0" minWidth="56.0" prefHeight="23.0" prefWidth="225.0" text="Status" textFill="#24545c">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="150.0" layoutY="233.0" mnemonicParsing="false" onAction="#Reset" prefHeight="35.0" prefWidth="79.0" text="Reset">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
